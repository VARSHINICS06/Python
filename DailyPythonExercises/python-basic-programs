#             print(i)
# sentence("python is programing ")

sentence = input("Enter a sentence: ")

upper = 0
lower = 0

for ch in sentence:
    if ch.isupper():
        upper += 1
    elif ch.islower():
        lower += 1

print("Uppercase letters:", upper)
print("Lowercase letters:", lower)





       


     

    
    
    

    

